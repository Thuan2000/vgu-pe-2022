{
	"name": "sdconnect-api",
	"version": "1.0.0",
	"description": "SDConnect API",
	"private": true,
	"scripts": {
		"test": "jest",
		"predb:migrate": "npx sequelize-cli db:migrate:undo:all",
		"db:migrate:seed": "npm-run-all db:migrate db:seed",
		"db:model:generate": "npx sequelize-cli model:generate",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:seed": "node src/db/seed",
		"lint": "eslint --fix ./src/**/*.ts",
		"compile:dev": "swc src --out-dir dist && npm run after:compile",
		"compile": "tsc && npm run after:compile",
		"after:compile": "tsc-alias --silent",
		"postafter:compile": "npm run copyschemas",
		"type-check": "tsc --pretty --skipLibCheck --noEmit",
		"type-check:watch": "tsc -w --pretty --skipLibCheck --noEmit",
		"start:dev": "npm-run-all clean compile:dev start:server",
		"watch": "cross-env NODE_ENV=development nodemon --watch src -e ts,graphql,json,env --exec npm run start:dev",
		"predev": "npm run codegen",
		"dev": "npm run --silent watch & npm run type-check:watch",
		"prestart": "npm run codegen",
		"start": "npm-run-all clean lint compile start:server",
		"start:server": "node ./dist/server.js",
		"clean": "rimraf dist/*",
		"precopyschemas": "mkdirp ./dist/graphql/schemas",
		"copyschemas": "copyfiles ./src/graphql/schemas/* ./dist/graphql/schemas/",
		"codegen": "graphql-codegen --config codegen.yml"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run lint",
			"...": "..."
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@elastic/elasticsearch": "^7.15.0",
		"@graphql-tools/load-files": "^6.4.0",
		"@graphql-tools/merge": "^8.1.2",
		"apollo-server-express": "^3.3.0",
		"aws-sdk": "^2.992.0",
		"bcrypt": "^5.0.1",
		"cross-env": "^7.0.3",
		"dotenv": "^8.1.0",
		"envalid": "^4.2.2",
		"express": "^4.17.1",
		"graphql": "^15.5.3",
		"graphql-import": "^1.0.2",
		"graphql-upload": "^12.0.0",
		"handlebars": "^4.7.7",
		"jsonwebtoken": "^8.5.1",
		"mysql": "^2.18.1",
		"mysql2": "^2.3.0",
		"nodemailer": "^6.6.3",
		"redis": "^3.1.2",
		"sequelize": "^6.6.5"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "2.2.0",
		"@graphql-codegen/import-types-preset": "^2.1.4",
		"@graphql-codegen/introspection": "^2.1.0",
		"@graphql-codegen/typescript": "2.2.2",
		"@graphql-codegen/typescript-resolvers": "2.2.1",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.105",
		"@types/bcrypt": "^3.0.0",
		"@types/dotenv": "^6.1.0",
		"@types/express": "^4.16.1",
		"@types/jest": "^24.0.18",
		"@types/jsonwebtoken": "^8.3.2",
		"@types/mock-req-res": "^1.1.0",
		"@types/mysql": "^2.15.19",
		"@types/nodemailer": "^6.4.4",
		"@types/redis": "^2.8.32",
		"@types/validator": "^13.6.3",
		"@typescript-eslint/eslint-plugin": "^2.3.3",
		"@typescript-eslint/parser": "^2.3.3",
		"copyfiles": "^2.4.1",
		"cpx": "^1.5.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-prettier": "^3.1.1",
		"graphql-let": "^0.18.5",
		"husky": "^3.0.9",
		"jest": "^24.9.0",
		"mkdirp": "^1.0.4",
		"mock-req-res": "^1.1.2",
		"nodemon": "^1.18.10",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4",
		"rimraf": "^2.6.3",
		"sinon": "^7.5.0",
		"ts-jest": "^24.1.0",
		"ts-node": "^8.4.1",
		"tsc-alias": "^1.3.9",
		"tsconfig-paths": "^3.11.0",
		"typescript": "^3.3.3"
	}
}
