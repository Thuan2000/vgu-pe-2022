{
	"name": "sdconnect-api",
	"version": "1.0.0",
	"description": "SDConnect API",
	"private": true,
	"scripts": {
		"test": "jest",
		"predb:migrate": "npx sequelize-cli db:migrate:undo:all",
		"db:migrate:seed": "npm-run-all db:migrate db:seed",
		"db:model:generate": "npx sequelize-cli model:generate",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:seed": "node src/db/seed",
		"codegen": "graphql-codegen --config codegen.yml",
		"lint": "eslint --fix ./src/**/*.ts",
		"nodemon": "cross-env NODE_ENV=development nodemon --watch dist -e js --exec npm-run-all --silent copyuncopied start:server",
		"dev:compile": "swc -w src --out-dir dist",
		"dev:type-check": "tsc -w --pretty --skipLibCheck --noEmit",
		"dev:watch": "npm run dev:compile & (sleep 1 && tsc-alias -w) & npm run dev:type-check",
		"predev": "npm-run-all clean codegen",
		"dev": "npm run --silent dev:watch & (sleep 4 && npm run nodemon)",
		"prestart": "npm run codegen",
		"build": "npm-run-all clean prod:compile",
		"build:no-lint": "npm-run-all clean prod:compile",
		"start": "npm-run-all build start:server",
		"prod:compile": "swc src --out-dir dist",
		"postprod:compile": "tsc-alias --silent && npm run copyuncopied",
		"start:server": "node ./dist/server.js",
		"clean": "rimraf dist/*",
		"precopyuncopied": "mkdirp ./dist/graphql/schemas && mkdirp ./dist/services/html-templates",
		"copyschemas": "cp ./src/graphql/schemas/* ./dist/graphql/schemas/",
		"copyemailtemplates": "cp ./src/html-templates/* ./dist/services/html-templates/",
		"copyuncopied": "npm-run-all copyschemas copyemailtemplates",
		"tsc": "tsc",
		"shop:pre-commit": "npm --prefix ../frontend/shop run type-check",
		"admin:pre-commit": "npm --prefix ../frontend/admin run type-check",
		"precommit": "npm-run-all shop:pre-commit admin:pre-commit tsc"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run precommit",
			"...": "..."
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@graphql-tools/load-files": "^6.4.0",
		"@graphql-tools/merge": "^8.1.2",
		"@opensearch-project/opensearch": "^1.0.0",
		"@types/sharp": "^0.29.3",
		"apollo-server-express": "^3.3.0",
		"async-redis": "^2.0.0",
		"aws-sdk": "^2.992.0",
		"base-64": "^1.0.0",
		"bcrypt": "^5.0.1",
		"cross-env": "^7.0.3",
		"dotenv": "^8.1.0",
		"envalid": "^4.2.2",
		"express": "^4.17.1",
		"graphql": "^15.5.3",
		"graphql-import": "^1.0.2",
		"graphql-upload": "^12.0.0",
		"handlebars": "^4.7.7",
		"jsonwebtoken": "^8.5.1",
		"lodash": "^4.17.21",
		"mysql": "^2.18.1",
		"mysql2": "^2.3.0",
		"node-cron": "^3.0.0",
		"nodemailer": "^6.6.3",
		"redis": "^3.1.2",
		"sequelize": "^6.6.5",
		"sequelize-hierarchy": "^2.0.4",
		"sharp": "^0.29.2",
		"tmp": "^0.2.1",
		"utf8": "^3.0.0",
		"websocket": "^1.0.34"
	},
	"devDependencies": {
		"@graphql-codegen/cli": "2.2.0",
		"@graphql-codegen/import-types-preset": "^2.1.4",
		"@graphql-codegen/introspection": "^2.1.0",
		"@graphql-codegen/typescript": "2.2.2",
		"@graphql-codegen/typescript-resolvers": "2.2.1",
		"@swc/cli": "^0.1.51",
		"@swc/core": "^1.2.105",
		"@types/async-redis": "^1.1.3",
		"@types/base-64": "^1.0.0",
		"@types/bcrypt": "^3.0.0",
		"@types/dotenv": "^6.1.0",
		"@types/express": "^4.16.1",
		"@types/jest": "^24.0.18",
		"@types/jsonwebtoken": "^8.3.2",
		"@types/lodash": "^4.14.181",
		"@types/mock-req-res": "^1.1.0",
		"@types/mysql": "^2.15.19",
		"@types/node-cron": "^3.0.1",
		"@types/nodemailer": "^6.4.4",
		"@types/redis": "^2.8.32",
		"@types/tmp": "^0.2.2",
		"@types/utf8": "^3.0.1",
		"@types/validator": "^13.6.3",
		"@typescript-eslint/eslint-plugin": "^2.3.3",
		"@typescript-eslint/parser": "^2.3.3",
		"copyfiles": "^2.4.1",
		"cpx": "^1.5.0",
		"eslint": "^6.5.1",
		"eslint-config-prettier": "^6.4.0",
		"eslint-plugin-prettier": "^3.1.1",
		"graphql-let": "^0.18.5",
		"husky": "^3.0.9",
		"jest": "^24.9.0",
		"mkdirp": "^1.0.4",
		"mock-req-res": "^1.1.2",
		"nodemon": "^1.18.10",
		"npm-run-all": "^4.1.5",
		"prettier": "^1.16.4",
		"rimraf": "^2.6.3",
		"sinon": "^7.5.0",
		"ts-jest": "^24.1.0",
		"ts-node": "^8.4.1",
		"tsc-alias": "^1.3.9",
		"tsconfig-paths": "^3.11.0",
		"typescript": "^3.3.3"
	}
}
