{
	"name": "sdconnect-api",
	"version": "1.0.0",
	"description": "SDConnect API",
	"private": true,
	"scripts": {
		"test": "jest",
		"predb:migrate": "npx sequelize-cli db:migrate:undo:all",
		"db:migrate": "npx sequelize-cli db:migrate",
		"db:seed": "npx sequelize-cli db:seed:all",
		"lint": "eslint --fix ./src/**/*.ts",
		"dev": "cross-env NODE_ENV=development nodemon --watch src -e ts --exec ts-node ./src/server.ts",
		"start": "npm-run-all clean lint compile start:server",
		"start:server": "node ./dist/server.js",
		"clean": "rimraf dist/*",
		"compile": "tsc"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm test && npm run lint",
			"...": "..."
		}
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"@graphql-tools/load-files": "^6.4.0",
		"@graphql-tools/merge": "^8.1.2",
		"apollo-server-express": "^3.3.0",
		"bcrypt": "^5.0.0",
		"cross-env": "^7.0.3",
		"dotenv": "^8.1.0",
		"envalid": "^4.2.2",
		"express": "^4.17.1",
		"graphql": "^15.5.3",
		"graphql-import": "^1.0.2",
		"graphql-upload": "^12.0.0",
		"jsonwebtoken": "^8.5.1",
		"mysql": "^2.18.1",
		"mysql2": "^2.3.0",
		"redis": "^3.1.2",
		"sequelize": "^6.6.5"
	},
