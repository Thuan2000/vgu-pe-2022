{
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "paths": {
      "@graphql/*": ["src/graphql/*"],
      "@utils/*": ["src/utils/*"],
      "@styles/*": ["src/styles/*"],
      "@settings/*": ["src/settings/*"],
      "@pages/*": ["src/pages/*"],
      "@components/*": ["src/components/*"],
      "@storybook/*": ["src/components/ui/storybook/*"],
      "@assets/*": ["src/assets/*"],
      "@datas/*": ["src/datas/*"]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/ui/storybook/image.tsx.save"
  ],
  "exclude": ["node_modules", ".cache"]
}
