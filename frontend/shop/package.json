{
  "name": "shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precodegen": "rm -rf src/graphql/*.d.ts",
    "codegen": "node -r dotenv/config $(yarn bin)/graphql-let && node -r dotenv/config $(yarn bin)/graphql-codegen",
    "predev": "rm -rf .next",
    "dev": "yarn codegen && cross-env NODE_ENV=development next dev",
    "build": "yarn codegen && next build",
    "start": "next start",
    "lint": "next lint",
    "pretype-check": "rm -rf src/graphql/*.d.ts && yarn codegen",
    "type-check": "tsc --skipLibCheck --noEmit",
    "pre-commit": "npm-run-all lint build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build",
      "...": "..."
    }
  },
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@babel/core": "^7.15.5",
    "@hookform/resolvers": "^2.8.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/graphql": "^14.5.0",
    "animated-number-react": "^0.1.2",
    "apollo-upload-client": "^16.0.0",
    "base-64": "^1.0.0",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.3",
    "graphql-tag": "^2.12.5",
    "javascript-time-ago": "^2.3.11",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "npm-run-all": "^4.1.5",
    "react": "17.0.2",
    "react-datepicker": "^4.3.0",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.4.0",
    "react-hook-form": "^7.15.3",
    "react-infinite-scroll-hook": "^4.0.1",
    "react-number-format": "^4.7.3",
    "react-phone-number-input": "^3.1.27",
    "react-select": "^5.1.0",
    "react-time-ago": "^7.1.9",
    "react-toastify": "^8.1.0",
    "react-use": "^17.3.1",
    "react-use-websocket": "^2.9.1",
    "sharp": "^0.29.1",
    "sweetalert2": "^11.1.5",
    "swiper": "^7.3.1",
    "utf8": "^3.0.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/import-types-preset": "^2.1.4",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.4",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@types/base-64": "^1.0.0",
    "@types/classnames": "^2.3.1",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^2.2.7",
    "@types/next-auth": "^3.15.0",
    "@types/react": "17.0.21",
    "@types/react-datepicker": "^4.1.7",
    "@types/react-phone-number-input": "^3.0.13",
    "@types/react-toastify": "^4.1.0",
    "@types/utf8": "^3.0.1",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "graphql-let": "^0.18.5",
    "husky": "^7.0.4",
    "next-i18next": "^8.8.0",
    "postcss": "^8.3.6",
    "tailwindcss": "^2.2.15",
    "tailwindcss-rtl": "^0.7.3",
    "typescript": "^4.4.3",
    "yaml-loader": "^0.6.0"
  }
}
