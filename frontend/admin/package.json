{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "precodegen": "rm -rf src/graphql/*.d.ts",
    "codegen": "node -r dotenv/config $(yarn bin)/graphql-let && node -r dotenv/config $(yarn bin)/graphql-codegen",
    "predev": "rm -rf .next",
    "build:dev": "env-cmd -f .env.development.local yarn build && yarn start",
    "build:prod": "env-cmd -f .env.production.local yarn build && yarn start",
    "dev": "yarn codegen && cross-env NODE_ENV=development PORT=3001 next dev",
    "build": "yarn codegen && next build",
    "start": "cross-env PORT=3001 next start",
    "start-docker": "next start",
    "lint": "next lint",
    "type:codegen": "graphql-codegen --config codegen.yml",
    "pretype-check": "rm -rf src/graphql/*.d.ts && yarn codegen",
    "type-check": "tsc --skipLibCheck --noEmit",
    "pre-commit": "npm-run-all build",
    "update-dotenv-local": "NODE_ENV=local node env-setter.js",
    "update-dotenv-dev": "NODE_ENV=dev node env-setter.js",
    "build-ecr-image-dev": "cd cloudformation-deployer && make build-deploy-image-dev",
    "update-ecs-dev": "cd cloudformation-deployer && node update-env-dev-json && make update-stack-dev",
    "predeploy-dev": "git pull",
    "deploy-dev": "npm run update-dotenv-dev && npm run build-ecr-image-dev && npm run update-ecs-dev"
  },
  "dependencies": {
    "@apollo/client": "^3.4.11",
    "@babel/core": "^7.15.5",
    "@hookform/resolvers": "^2.8.0",
    "@types/apollo-upload-client": "^14.1.0",
    "@types/graphql": "^14.5.0",
    "animated-number-react": "^0.1.2",
    "apollo-upload-client": "^16.0.0",
    "base-64": "^1.0.0",
    "classnames": "^2.3.1",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.3",
    "graphql-tag": "^2.12.5",
    "javascript-time-ago": "^2.3.13",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "next": "^11.1.2",
    "next-auth": "^3.29.0",
    "npm-run-all": "^4.1.5",
    "react": "17.0.2",
    "react-checkmark": "^1.4.0",
    "react-cropper": "^2.1.8",
    "react-datepicker": "^4.2.1",
    "react-device-detect": "^2.1.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hook-form": "^7.15.3",
    "react-number-format": "^4.7.3",
    "react-phone-number-input": "^3.1.27",
    "react-select": "^5.0.0",
    "react-time-ago": "^7.1.9",
    "react-toastify": "^8.1.0",
    "react-use": "^17.3.1",
    "sharp": "^0.29.1",
    "sweetalert2": "^11.1.5",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/import-types-preset": "^2.1.4",
    "@graphql-codegen/introspection": "^2.1.0",
    "@graphql-codegen/typescript": "2.2.2",
    "@graphql-codegen/typescript-operations": "2.1.4",
    "@graphql-codegen/typescript-react-apollo": "3.1.4",
    "@types/base-64": "^1.0.0",
    "@types/classnames": "^2.3.1",
    "@types/cookie": "^0.4.1",
    "@types/js-cookie": "^2.2.7",
    "@types/lodash": "^4.14.175",
    "@types/next-auth": "^3.15.0",
    "@types/react": "17.0.21",
    "@types/react-cropper": "^1.3.1",
    "@types/react-datepicker": "^4.1.7",
    "@types/react-phone-number-input": "^3.0.13",
    "@types/react-toastify": "^4.1.0",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "env-cmd": "^10.1.0",
    "eslint": "7.32.0",
    "eslint-config-next": "11.1.2",
    "graphql-let": "^0.18.5",
    "next-i18next": "^8.8.0",
    "postcss": "^8.3.6",
    "pre-commit": "^1.2.2",
    "tailwindcss": "^2.2.15",
    "tailwindcss-rtl": "^0.7.3",
    "typescript": "^4.4.4",
    "yaml-loader": "^0.6.0"
  }
}
